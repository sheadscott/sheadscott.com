{
  "name": "nextjs-site",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "yarn audit && ENV=dev next -p 7777",
    "build": "next build",
    "create-sendgrid-secret": "now secret add sendgrid-api-key YOUR_API_KEY_HERE",
    "start": "next start",
    "export": "next export",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "now-build": "next build",
    "devdeploy": "now alias $(now) sheadscott-dev",
    "deploy": "now"
  },
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@mdx-js/mdx": "^0.15.0",
    "@sendgrid/mail": "^6.3.1",
    "@zeit/next-mdx": "1.2.0",
    "axios": "^0.18.0",
    "dotenv-webpack": "^1.7.0",
    "html-react-parser": "0.6.2",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^6.0.0",
    "imagemin-svgo": "^7.0.0",
    "next": "8.0.3",
    "next-compose-plugins": "^2.1.1",
    "next-optimized-images": "^2.3.3",
    "now": "^14.0.3",
    "preact": "^8.4.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-json-view": "^1.19.1",
    "remark-images": "^0.17.0",
    "responsive-loader": "^1.2.0",
    "sharp": "^0.21.3",
    "styled-components": "^4.1.3",
    "webp-loader": "^0.5.0",
    "webpack": "^4.29.3"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-styled-components": "^1.10.0"
  },
  "resolutions": {
    "babel-plugin-inline-react-svg/svgo/js-yaml": "^3.13.1"
  },
  "peerDependencies": {},
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      "inline-react-svg",
      "@babel/plugin-syntax-dynamic-import",
      [
        "styled-components",
        {
          "ssr": true
        }
      ]
    ]
  }
}
